{
  "name": "nodets",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
 "scripts": {
  "dev": "bun --watch src/index.ts", 
  "build": "tsc",
  "start": "bun run dist/index.js",
  "lint": "bunx eslint src/**/*.{ts,js}",
  "lint:fix": "bunx eslint . --fix",
  "format": "bunx prettier --write 'src/**/*.{ts,js,json,md}'"
},
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bunx eslint --fix",
      "bunx prettier --write"
    ],
    "*.{json,yml,yaml,md}": [
      "bunx prettier --write"
    ]
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "nodeman": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
